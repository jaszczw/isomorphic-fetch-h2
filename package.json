{
  "name": "isomorphic-fetch-h2",
  "version": "0.0.0",
  "description": "Isomorphic WHATWG Fetch API, for Node & Browserify with fetch-h2 on node",
  "browser": "fetch-npm-browserify.js",
  "main": "fetch-npm-node.js",
  "scripts": {
    "files": "find . -name '*.js' ! -path './node_modules/*' ! -path './bower_components/*'",
    "test": "jshint `npm run -s files` && lintspaces -i js-comments -e .editorconfig `npm run -s files` && mocha"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jaszczw/isomorphic-fetch-h2.git"
  },
  "author": "Matt Andrews <matt@mattandre.ws>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jaszczw/isomorphic-fetch-h2/issues"
  },
  "homepage": "https://github.com/jaszczw/isomorphic-fetch-h2/issues",
  "dependencies": {
    "fetch-h2": "^2.2.0",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "chai": "^1.10.0",
    "es6-promise": "^2.0.1",
    "jshint": "^2.5.11",
    "lintspaces-cli": "^0.1.1",
    "mocha": "^2.1.0",
    "nock": "^0.59.1",
    "npm-prepublish": "^1.0.2"
  }
}
